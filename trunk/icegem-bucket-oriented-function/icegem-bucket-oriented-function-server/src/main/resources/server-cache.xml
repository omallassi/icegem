<?xml version="1.0"?>
<!DOCTYPE cache PUBLIC
        "-//GemStone Systems, Inc.//GemFire Declarative Caching 6.5//EN"
        "http://www.gemstone.com/dtd/cache6_5.dtd">

<cache>
    <!--Port will be set automatically. It helps to start servers based on one cache.xml-->
    <cache-server port="0"/>

    <region name="data" refid="PARTITION">
        <region-attributes>
            <partition-attributes redundant-copies="1"/>
            <cache-listener>
                <class-name>com.googlecode.icegem.core.functions.bucketoriented.common.plugins.SimpleCacheListener</class-name>
            </cache-listener>
        </region-attributes>
    </region>
</cache>
