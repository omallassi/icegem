<?xml version="1.0"?>
<!DOCTYPE client-cache PUBLIC
        "-//GemStone Systems, Inc.//GemFire Declarative Caching 6.5//EN"
        "http://www.gemstone.com/dtd/cache6_5.dtd">

<client-cache>
    <pool name="client" subscription-enabled="true">
        <locator host="localhost" port="10355"/>
    </pool>

    <region name="data" refid="PROXY">
        <region-attributes pool-name="client">
            <cache-listener>
                <class-name>com.googlecode.icegem.core.functions.bucketoriented.common.plugins.SimpleCacheListener</class-name>
            </cache-listener>
        </region-attributes>
    </region>
</client-cache>
