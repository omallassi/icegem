<?xml version="1.0"?>
<!DOCTYPE client-cache PUBLIC
    "-//GemStone Systems, Inc.//GemFire Declarative Caching 6.5//EN"
    "http://www.gemstone.com/dtd/cache6_5.dtd">
<client-cache>
    <pool name="msg-buf-pool" subscription-enabled="true">
        <locator host="localhost" port="10355"/>
        <!--<locator host="gemshunter-vm6.vm.griddynamics.net" port="12345"/>-->
    </pool>
    <region name="trades">
        <region-attributes pool-name="msg-buf-pool">
            <cache-listener>
                <class-name>com.googlecode.icegem.message.barrier.core.plugins.BarrierEntityRegionListener</class-name>
            </cache-listener>
        </region-attributes>
    </region>
    <region name="msgs" refid="PROXY">
        <region-attributes pool-name="msg-buf-pool">
            <cache-listener>
                <class-name>com.googlecode.icegem.message.barrier.core.plugins.BarrierMessageRegionListener</class-name>
            </cache-listener>
        </region-attributes>
    </region>
    <region name="msgs-sequence" refid="LOCAL">
        <region-attributes scope="global">
        </region-attributes>
    </region>
    <region name="expired-msgs" refid="PROXY">
        <region-attributes pool-name="msg-buf-pool"/>
    </region>
    <!--<region name="msg-to-del" refid="LOCAL">
        &lt;!&ndash;<region-attributes>
            <cache-listener>
                <class-name>AdapterMsgToDelListener</class-name>
            </cache-listener>
        </region-attributes>&ndash;&gt;
    </region>-->
    <region name="msgToCheck" refid="LOCAL">
        <region-attributes scope="global">
        </region-attributes>
    </region>
</client-cache>