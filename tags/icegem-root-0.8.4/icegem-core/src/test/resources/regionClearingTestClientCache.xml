<?xml version="1.0"?>
<!DOCTYPE client-cache PUBLIC
        "-//GemStone Systems, Inc.//GemFire Declarative Caching 6.5//EN"
        "http://www.gemstone.com/dtd/cache6_5.dtd">
<client-cache>
    <pool name="client" subscription-enabled="true">
        <locator host="localhost" port="${locator.port}"/>
    </pool>
    <region name="partitioned_region_1">
        <region-attributes pool-name="client" refid="PROXY"/>
    </region>
    <region name="replicated_region_1">
        <region-attributes pool-name="client" refid="PROXY"/>
    </region>
    <region name="partitioned_region_2">
        <region-attributes pool-name="client" refid="CACHING_PROXY"/>
    </region>
    <region name="replicated_region_2">
        <region-attributes pool-name="client" refid="CACHING_PROXY"/>
    </region>
</client-cache>

